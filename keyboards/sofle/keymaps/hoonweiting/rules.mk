# Enable mouse keys
MOUSEKEY_ENABLE = yes
# Enable audio control and system control
EXTRAKEY_ENABLE = yes
# Enable encoders
ENCODER_ENABLE = yes
ENCODER_MAP_ENABLE = yes
# Enable caps word
CAPS_WORD_ENABLE = yes
# Link Time Optimization
LTO_ENABLE = yes
# Sofle uses atmega32u4 by default
CONVERT_TO = promicro_rp2040
# Enable configuration through VIA
VIA_ENABLE = yes
